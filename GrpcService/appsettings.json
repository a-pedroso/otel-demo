{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },

  "OpenTelemetryConfig": {
    "Enabled": true,
    "OtlpExporter": {
      "AgentUrl": "http://localhost:4318"
    }
  },

  "ConnectionStrings": {
    "Redis": "localhost:6379"
  }
}
